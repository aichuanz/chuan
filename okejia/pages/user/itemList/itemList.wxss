/* pages/user/itemList/itemList.wxss */
page{
background: #f7f7f7;
}
.itemList{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.list{
  width: 710rpx;
   margin-top: 20rpx;
   margin-left: 20rpx;
}
.list .list-name{
 height: 90rpx;
 line-height: 90rpx; 
 background: #FFFFFF;
 border-radius: 10px;
 border-bottom: 2rpx dashed #e5e5e5;
}
.list .list-name text{
  margin-left: 20rpx;
  font-size: 28rpx;
color: #333333;
letter-spacing: 0;
}
.list .list-content{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;

  border-radius: 10px;
  background: #FFFFFF;
}
.list-content .contents{
  width: 646rpx;
  height: 200rpx;
  margin-top: 30rpx;
  margin-left: 32rpx;
  
}
.list-content .contents text{
  font-size: 30rpx;
  color: #666666;
  letter-spacing: 0;
}
.list-content .date{
  height: 83rpx;
  line-height: 83rpx;
  margin-left: 442rpx;
  font-size: 24rpx;
color: #666666;
letter-spacing: 0;
}